<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://maven.apache.org/POM/4.0.0"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <artifactId>rno-lte-pci-afp-service</artifactId>
    <packaging>jar</packaging>

    <name>rno-lte-pci-afp-service</name>
    <description>Project for RNO PCI AFP Microservices</description>

    <parent>
        <groupId>com.hgicreate.rno.lte</groupId>
        <artifactId>rno-lte-service</artifactId>
        <version>1.0.0.M1</version>
        <relativePath>..</relativePath> <!-- lookup parent from repository -->
