.td-style {
    float: left;
    display:inline-block;
}

.td-space {
    padding-right: 60px;
}

.td-padding {
    padding-top: 12px;
}

.td-left-space {
    padding-left: 60px;
}

.td-right-space {
    padding-right: 60px;
}

.vertical-style {
    vertical-align: top;
}

.td-width {
    width: 50px;
}

.star-style {
    color: red;
    font-size: 18px;
}

.btn-width1 {
    width: 50px;
    margin: 5px 10px 0;
}

.page-style {
    float: right;
}

.paging_input_text {
    display: inline-block;
    height: 18px;
    vertical-align: middle;
    line-height: 15px;
    padding: 3px 1px 2px 1px;
    width: 60px;
    text-align: center;
}

.fix-div {
    position: fixed;
    width: 100%;
    z-index: 1000;
    background-color: white;
    overflow: hidden;
}

.bs-div {
    padding-top: 76px;
}

.loading {
    width: 160px;
    height: 56px;
    position: fixed;
    top: 50%;
    left: 45%;
    line-height: 56px;
    color: #fff;
    padding-left: 60px;
    font-size: 15px;
    background: #000 url(../img/loader.gif) no-repeat 10px 50%;
    opacity: 0.7;
    z-index: 9999;
    -moz-border-radius: 20px;
    -webkit-border-radius: 20px;
    border-radius: 20px;
    filter: progid:DXImageTransform.Microsoft.Alpha(opacity=70);
}

.info-style {
    width: 200px;
    height: 56px;
    position: fixed;
    top: 50%;
    left: 45%;
    line-height: 56px;
    color: #fff;
    text-align: center;
    font-size: 15px;
    background: #ff0000;
    filter: alpha(opacity=100);
    -moz-opacity: 1.0;
    opacity: 1.0;
    z-index: 9999;
    -moz-border-radius: 10px;
    -webkit-border-radius: 10px;
    border-radius: 10px;
}

.btn-width {
    width: 80px;
}

.pagination > li:first-child > div {
    border-bottom-left-radius: 4px;
}

.pagination > li:last-child > a, .pagination > li:last-child > span {
    border-top-right-radius: 0;
    border-bottom-right-radius: 4px;
}

.span-style {
    position: relative;
    float: left;
    padding: 6px 12px;
    margin-left: -1px;
    line-height: 1.42857143;
    color: #337ab7;
    text-decoration: none;
    background-color: #fff;
    border: 1px solid #ddd;
}

.form-bottom-style {
    margin: 0 0 0;
    overflow-x:auto;
white-space: nowrap;
}

.label-style {
    font-weight: normal;
}

.label-style1 {
    white-space: nowrap;
    overflow: hidden;
    font-weight: normal;
}

.label-style2 {
    font-weight: normal;
    width:80px;
    text-align:right;
}

.label-style3 {
    font-weight: normal;
    width:80px;
    text-align:left;
}

.td-top-align {
    height: 156px;
    width: 180px;
    vertical-align: top;
    padding-top: 5px;
}

.td-middle-align {
    vertical-align: middle;
   padding-top: 6px;
}

.td-bottom-align {
    height: 156px;
    vertical-align: bottom;
}

.select-style {
    width: 150px;
    display: inline;
}

textarea {
    resize: none;
}

.table-style tr td{
	margin-bottom:5px;
	display:inline-block;
}

.div-larger{
	width:-moz-max-content;
}
